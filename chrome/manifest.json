{
  "name": "FrF Helpers",
  "version": "1.4.3",
  "manifest_version": 2,
  "description": "FriendFeed image preview helpers",
  "icons": { "128": "Icon.png" },
  "permissions": [ "storage" ],
  "web_accessible_resources": [ "jquery.js", "jquery-ui-1.10.1.custom.min.js", "frf.js", "frf-base.js", "frf.css", "view.html", "view.js" ],
  "content_scripts": [
    {
      "matches": [ "http://friendfeed.com/*" ],
      "css": [ "frf.css" ],
      "js": [ "jquery.js", "jquery-ui-1.10.1.custom.min.js", "frf.js", "frf-base.js" ]
    }
  ],
  "options_page": "settings.html"
}